<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="none"
    android:layout_weight="1">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TableLayout
        android:id="@+id/tl"
        android:layout_marginTop="30dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="7">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Excute Time(s)"
                android:textSize="16dp" />
            <EditText
                android:id="@+id/exe_time"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="5"
                android:layout_weight="1"
                android:phoneNumber="true"
                />
            </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:id="@+id/textView5"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="16dp"
                android:text="Delta"
                />

            <EditText
                android:id="@+id/delta_val"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="50"
                android:textSize="16dp"
                android:layout_weight="1"
                android:phoneNumber="true"
                />
        </TableRow>
        <!-- All text View  -->
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Elapased Time:"
                android:textSize="16dp"
                android:layout_weight="1"
                />
            <TextView
                android:id="@+id/ela_time"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:textSize="16dp"
                android:layout_weight="1"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Current Frame:"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/current_frame"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:textSize="16dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Current Frequency(KHz)"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/current_freq"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:textSize="16dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Current Frequency Seed"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/current_freq_seed"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:textSize="16dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Number of Failed Frame:"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/failframe_num"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:textSize="16dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Failed Rate:"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/fail_rate"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:textSize="16dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="File Storage:"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/file_path"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:textSize="16dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Frequency(KHz)"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/total_freq_show"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:textSize="16dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2" >
            <TextView
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="Status"
                android:layout_weight="1"
                android:textSize="16dp"
                />
            <TextView
                android:id="@+id/status"
                android:width="0dp"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                />
        </TableRow>
        <!-- last button line  -->
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0.5dp">
            <Button
                android:id="@+id/start_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="START"
                android:layout_weight="1"
                />
            <Button
                android:id="@+id/back_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BACK"
                android:layout_weight="1"
                />
        </TableRow>

    </TableLayout>
    <TableLayout
        android:id="@+id/t2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="7">

    </TableLayout>

